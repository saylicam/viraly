{
  "name": "viraly",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "cross-env EXPO_DEV_SERVER_LISTEN_ADDRESS=0.0.0.0 expo start --host lan",
    "start:lan": "cross-env EXPO_DEV_SERVER_LISTEN_ADDRESS=0.0.0.0 expo start --host lan",
    "start:lan:clear": "cross-env EXPO_DEV_SERVER_LISTEN_ADDRESS=0.0.0.0 expo start --host lan --clear",
    "start:tunnel": "cross-env EXPO_DEV_SERVER_LISTEN_ADDRESS=0.0.0.0 expo start --host tunnel",
    "start:lan:ip": "cross-env REACT_NATIVE_PACKAGER_HOSTNAME=192.168.0.12 EXPO_DEV_SERVER_LISTEN_ADDRESS=0.0.0.0 expo start --host lan --clear",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build": "expo build",
    "eject": "expo eject",
    "setup": "node scripts/setup.js",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd src/server && npm run dev",
    "dev:frontend": "cross-env EXPO_DEV_SERVER_LISTEN_ADDRESS=0.0.0.0 expo start --host lan",
    "install:all": "npm install && cd src/server && npm install",
    "doctor": "expo doctor --fix-dependencies"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "@react-navigation/stack": "^6.4.1",
    "expo": "~54.0.30",
    "expo-apple-authentication": "~8.0.8",
    "expo-asset": "~12.0.9",
    "expo-auth-session": "~7.0.10",
    "expo-av": "~16.0.8",
    "expo-blur": "~15.0.8",
    "expo-camera": "~17.0.10",
    "expo-clipboard": "~8.0.8",
    "expo-constants": "~18.0.10",
    "expo-file-system": "~19.0.17",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.8",
    "expo-image-picker": "~17.0.10",
    "expo-linear-gradient": "~15.0.8",
    "expo-media-library": "~18.2.1",
    "expo-status-bar": "~3.0.9",
    "expo-web-browser": "~15.0.10",
    "firebase": "^12.6.0",
    "lucide-react-native": "^0.554.0",
    "moti": "^0.30.0",
    "motti": "^1.0.1",
    "nativewind": "^2.0.11",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-animatable": "^1.4.0",
    "react-native-calendars": "^1.1313.0",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-purchases": "^9.6.11",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-worklets": "0.5.1",
    "recharts": "^3.3.0",
    "tailwindcss": "3.3.2",
    "uuid": "^9.0.1",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@types/react": "~19.1.10",
    "@types/uuid": "^9.0.7",
    "babel-preset-expo": "^54.0.6",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "typescript": "~5.9.2"
  },
  "resolutions": {
    "react-native-reanimated": "~4.1.1"
  },
